<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"  package="aplimoviles.com.popshow" android:targetSandboxVersion="1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:screenOrientation="portrait"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/AppTheme">

        <activity android:name=".SplashActivity">
             <intent-filter>
                    <action android:name="android.intent.action.MAIN"/>
                     <category android:name="android.intent.category.LAUNCHER"/>
             </intent-filter>
        </activity>

        <activity android:name=".Principal"
            android:screenOrientation="portrait"
            android:label="@string/PopShow" />

        <activity
            android:name=".Caldas"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_caldas" />

        <activity android:name=".MuseoHN"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_MuseoHn"/>

        <activity android:name=".Ermita"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_ermita" />

        <activity android:name=".Morro"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_morro" />

        <activity android:name=".Campanario"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_campanario" />

        <activity android:name=".Pueblito"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_pueblitop" />

        <activity android:name=".favoritos"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_favoritos"/>



    </application>

</manifest>